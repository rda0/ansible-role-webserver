log_format vhost_combined_tls_upstream escape=none '[$time_local] $host:$server_port' '$upstream_cache_status : "$remote_addr" -> "$proxy_host" -> "$upstream_addr" - "$remote_user" "$http_x_forwarded_for" "$request" $status $body_bytes_sent $request_length $request_time "$http_referer" "$http_user_agent" $ssl_protocol $ssl_cipher';
log_format vhost_combined_tls_upstream_withreq escape=none '[$time_local] $host:$server_port' '$upstream_cache_status : "$remote_addr" -> "$proxy_host" -> "$upstream_addr" - "$remote_user" "$http_x_forwarded_for" "$request" $status $body_bytes_sent $request_length $request_time "$http_referer" "$http_user_agent" $ssl_protocol $ssl_cipher :: $request_body';
log_format synapse_admins_ricardo_duarte_withuri escape=none '[$time_local] $upstream_cache_status : "$remote_addr" -> "$proxy_host" -> "$upstream_addr" "$request" - $http_x_forwarded_for $status - "$http_referer" "$http_user_agent"';
